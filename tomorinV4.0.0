local SnowEffect = nil
local SnowEnabled = false

-- ç²‰è‰²é£˜é›ªæ•ˆæœ
local function CreateSnowEffect(parent)
    if SnowEffect then
        SnowEffect:Destroy()
    end
    
    local SnowGui = Instance.new("ScreenGui")
    SnowGui.Name = "PinkSnowEffect"
    SnowGui.ZIndexBehavior = Enum.ZIndexBehavior.Sibling
    SnowGui.ResetOnSpawn = false
    
    -- åˆ›å»ºå¤šä¸ªé›ªèŠ±
    local snowflakes = {}
    local function CreateSnowflake()
        local snowflake = Instance.new("Frame")
        local size = math.random(4, 12)
        snowflake.Size = UDim2.new(0, size, 0, size)
        snowflake.Position = UDim2.new(0, math.random(-100, 1200), 0, -20)
        snowflake.BackgroundColor3 = Color3.fromRGB(255, 182, 193) -- ç²‰çº¢è‰²
        snowflake.BorderSizePixel = 0
        snowflake.BackgroundTransparency = math.random(2, 8) / 10
        snowflake.Parent = SnowGui
        
        local corner = Instance.new("UICorner")
        corner.CornerRadius = UDim.new(1, 0)
        corner.Parent = snowflake
        
        table.insert(snowflakes, {
            Frame = snowflake,
            Speed = math.random(20, 60),
            Swing = math.random(1, 10),
            SwingSpeed = math.random(1, 5) / 100,
            Size = size
        })
    end
    
    -- åˆ›å»ºåˆå§‹é›ªèŠ±
    for i = 1, 100 do
        CreateSnowflake()
    end
    
    -- é›ªèŠ±åŠ¨ç”»
    local connection
    connection = game:GetService("RunService").RenderStepped:Connect(function(delta)
        if not SnowEnabled then
            connection:Disconnect()
            return
        end
        
        for i, snowflake in ipairs(snowflakes) do
            local currentX = snowflake.Frame.Position.X.Offset
            local currentY = snowflake.Frame.Position.Y.Offset
            
            -- ä¸‹è½
            local newY = currentY + snowflake.Speed * delta
            
            -- æ‘†åŠ¨æ•ˆæœ
            local swingOffset = math.sin(tick() * snowflake.SwingSpeed + snowflake.Swing) * 3
            
            -- å¦‚æœé›ªèŠ±è½åˆ°åº•éƒ¨ï¼Œé‡æ–°ä»é¡¶éƒ¨å¼€å§‹
            if newY > 800 then
                newY = -20
                currentX = math.random(-100, 1200)
            end
            
            snowflake.Frame.Position = UDim2.new(0, currentX + swingOffset, 0, newY)
            
            -- è½»å¾®æ—‹è½¬æ•ˆæœ
            snowflake.Frame.Rotation = math.sin(tick() * snowflake.SwingSpeed) * 10
        end
        
        -- å¶å°”æ·»åŠ æ–°é›ªèŠ±
        if math.random(1, 15) == 1 and #snowflakes < 120 then
            CreateSnowflake()
        end
    end)
    
    SnowGui.Parent = parent
    SnowEffect = SnowGui
    SnowEnabled = true
    
    return SnowGui
end

local function RemoveSnowEffect()
    SnowEnabled = false
    if SnowEffect then
        SnowEffect:Destroy()
        SnowEffect = nil
    end
end

